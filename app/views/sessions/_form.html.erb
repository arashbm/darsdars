<%= simple_form_for(@session) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :course_id %>
    <%= f.input :start %>
    <%= f.input :finish %>
    <%= f.input :instructor %>
    <%= f.input :session_type %>
    <%= f.input :weekday %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
